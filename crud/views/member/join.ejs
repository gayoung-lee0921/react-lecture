<%- include("../include/sub-header")%>
<!-- 서버로 내용을 전달하는 방법 1. ajax(페이지 유지된 채로), 2.form(페이지 새로 바뀌면서)  -->
<div class="container">
  <!-- 내용을 보낼 때는 method=post, 값을 얻어올 땐 method=get-->
  <form action="/member/join" id="write-form" method="post">
    <div class="bbs write">
      <div class="row">
        <span>아이디</span>
        <input type="text" id="id" name="id" />
      </div>
      <div class="row">
        <span>패스워드</span>
        <input type="password" id="password" name="password" />
      </div>
      <div class="row">
        <span>이름</span>
        <input type="text" id="name" name="name" />
      </div>
      <div class="row">
        <span>H.P</span>
        <input type="text" id="phone" name="phone" />
      </div>
      <div class="row">
        <span>주소</span>
        <input type="text" id="address" name="address" />
      </div>
      <div class="btns">
        <button class="btn btn-write">등록</button>
        <button class="btn btn-reset" type="reset">되돌아가기</button>
      </div>
    </div>
  </form>
</div>
<%- include("../include/sub-footer") %>
